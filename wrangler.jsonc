   "compatibility_date": "2025-07-18",
   "assets": {
      "not_found_handling": "single-page-application"
   },
   "observability": {
      "enabled": true
   },
  "routes": [
    {
      "pattern": "barroit.sh",
      "custom_domain": true
    }
  ]
}
